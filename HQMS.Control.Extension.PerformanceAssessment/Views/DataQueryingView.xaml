             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
             xmlns:local="clr-namespace:HQMS.Control.Extension.PerformanceAssessment.Views"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <Grid Margin="16">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <StackPanel Grid.Row="0"
                    Orientation="Horizontal">
            <TextBlock Text="开始日期"
                       VerticalAlignment="Center"
                       Style="{DynamicResource MaterialDesignTextBlock}"/>
            <DatePicker Margin="8 0 0 0"
                        materialDesign:CalendarAssist.IsHeaderVisible="False"
                        Width="100"
                        SelectedDate="{Binding DataQueryingModel.BeginDate,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"
                        SelectedDateFormat="Short"/>
            <TextBlock Text="结束日期"
                       Margin="32 0 0 0"
                       VerticalAlignment="Center"
                       Style="{DynamicResource MaterialDesignTextBlock}"/>
            <DatePicker Margin="8 0 0 0"
                        materialDesign:CalendarAssist.IsHeaderVisible="False"
                        Width="100"
                        SelectedDate="{Binding DataQueryingModel.EndDate,Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}"/>

            <Button Margin="32 0 0 0"
                    Style="{DynamicResource MaterialDesignOutlinedLightButton}"
                    Content="查询"
                    Command="{Binding QueryCommand}"/>

            <Button Margin="32 0 0 0"
                    Style="{DynamicResource MaterialDesignOutlinedLightButton}"
                    Content="导出"
                    Command="{Binding ExportCommand}"/>

            <Button Margin="32 0 0 0"
                    Style="{DynamicResource MaterialDesignOutlinedLightButton}"
                    Content="上传"
                    Command="{Binding UpLoadCommand}"/>
        </StackPanel>

        <StackPanel Grid.Row="1">
            <DataGrid ItemsSource="{Binding DataQueryingModel.Results, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                      CanUserAddRows="False"
                      CanUserDeleteRows="False"
